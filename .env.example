# DeployGenie Environment Configuration

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api

# GitHub OAuth Configuration
VITE_GITHUB_CLIENT_ID=your_github_client_id_here

# Stripe Configuration (for billing)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_MONITORING=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_BILLING=true
VITE_ENABLE_HEALTH_CHECKS=true

# Deployment Configuration
VITE_DEFAULT_CLOUD_PROVIDER=vercel
VITE_DEFAULT_REGION=us-east-1

# Logging and Error Tracking
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_LOG_LEVEL=info

# WebSocket Configuration (for real-time updates)
VITE_WS_URL=ws://localhost:3001

# CDN Configuration
VITE_CDN_URL=https://cdn.deploygenie.com

# Support and Documentation
VITE_SUPPORT_EMAIL=support@deploygenie.com
VITE_DOCS_URL=https://docs.deploygenie.com
